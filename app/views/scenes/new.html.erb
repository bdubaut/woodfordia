<%= form_for :scene, url: {action: "create"} do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>
  <br>
  <%= f.label :description %>
  <%= f.text_area :description %>
  <br>
  <%= f.label :question_1 %>
  <%= f.text_field :question_1 %>
  <br>
  <%= f.label :question_2 %>
  <%= f.text_field :question_2 %>
  <br>
  <%= f.label :question_3 %>
  <%= f.text_field :question_3 %>
  <br>
  <%= f.label :previous_scenes %>
  <%= f.select('previous_scenes', options_for_select(@list),{}, { multiple: true}) %>
  <br>
  <%= f.label :next_scenes %>
  <%= f.select('next_scenes', options_for_select(@list), {}, { multiple: true }) %>
  <br>
  <%= f.submit 'Create Scene' %>
<% end %>
